{
  "name": "fiap-final",
  "version": "1.0.0",
  "main": "index.js",
  "author": "hugokishi <hugopacheco370@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/app/index.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/app/index.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:generate": "ts-node-dev ./node_modules/.bin/typeorm migration:generate -n",
    "migration:create": "ts-node-dev ./node_modules/.bin/typeorm migration:create -n",
    "migration:revert": "ts-node-dev ./node_modules/.bin/typeorm migration:revert",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@types/express-session": "^1.17.4",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/morgan": "^1.9.3",
    "@types/redis": "^2.8.31",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "jest": "^27.0.6",
    "jest-extended": "^0.11.5",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}